<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">

    <f:entry title="${%Include Pattern}" help="${rootURL}/../plugin/testlink/help-junitIncludePattern.html">
        <f:textbox id="includePattern" name="JUnitCaseNameResultSeeker.includePattern" value="${instance.includePattern}" />
    </f:entry>

    <f:entry title="${%Key Custom Field}" help="${rootURL}/../plugin/testlink/help-atKeyCustomField.html">
        <f:textbox id="keyCustomField" name="JUnitCaseNameResultSeeker.keyCustomField" value="${instance.keyCustomField}" />
    </f:entry>
    
    <f:entry title="${%Filter Executed by Keywords}" help="${rootURL}/../plugin/testlink/help-atkeywordsExecutedFilter.html">
        <f:textbox id="keywordsExecutedFilter" name="TestNGMethodNameResultSeeker.keywordsExecutedFilter" value="${instance.keywordsExecutedFilter}" />
    </f:entry>
    
    <f:entry title="${%Attach JUnit XML}" help="${rootURL}/../plugin/testlink/help-attachResult.html">
        <f:checkbox name="JUnitCaseNameResultSeeker.attachJUnitXML" value="${instance.attachJUnitXML}" checked="${instance.attachJUnitXML}" />
    </f:entry>
    
    <f:entry title="${%Include test notes}" help="${rootURL}/../plugin/testlink/help-includeNotes.html">
        <f:checkbox name="JUnitCaseNameResultSeeker.includeNotes" value="${instance.includeNotes}" checked="${instance.includeNotes}" />
    </f:entry>

</j:jelly>