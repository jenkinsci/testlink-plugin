<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">

    <f:entry title="${%Include Pattern}" help="${rootURL}/../plugin/testlink/help-testNGIncludePattern.html">
        <f:textbox id="includePattern" name="TestNGMethodNameResultSeeker.includePattern" value="${instance.includePattern}" />
    </f:entry>

    <f:entry title="${%Key Custom Field}" help="${rootURL}/../plugin/testlink/help-atKeyCustomField.html">
        <f:textbox id="keyCustomField" name="TestNGMethodNameResultSeeker.keyCustomField" value="${instance.keyCustomField}" />
    </f:entry>
    
     <f:entry title="${%Filter Keywords Executed}" help="${rootURL}/../plugin/testlink/help-atKeywordExdFilter.html">
        <f:textbox id="KeywordExdFilter" name="TestNGMethodNameResultSeeker.KeywordExdFilter" value="${instance.KeywordExdFilter}" />
    </f:entry>      
        
    <f:entry title="${%Attach TestNG XML}" help="${rootURL}/../plugin/testlink/help-attachResult.html">
        <f:checkbox name="TestNGMethodNameResultSeeker.attachTestNGXML" value="${instance.attachTestNGXML}" checked="${instance.attachTestNGXML}" />
    </f:entry>  
    
    <f:entry title="${%Attach pdf report}" help="${rootURL}/../plugin/testlink/help-attachPdfReport.html">
        <f:checkbox name="TestNGMethodNameResultSeeker.attachPdfReport" value="${instance.attachPdfReport}" checked="${instance.attachPdfReport}" />
    </f:entry>
        
    <f:entry title="${%Report testCasesReportFolder}" help="${rootURL}/../plugin/testlink/help-testCasesReportFolder.html">
        <f:textbox id="testCasesReportFolder" name="TestNGMethodNameResultSeeker.testCasesReportFolder" value="${instance.testCasesReportFolder}" />
    </f:entry>
    
    <f:entry title="${%Mark skipped test as Blocked}" help="${rootURL}/../plugin/testlink/help-markSkippedTestAsBlocked.html">
        <f:checkbox name="TestNGClassNameResultSeeker.markSkippedTestAsBlocked" value="${instance.markSkippedTestAsBlocked}" checked="${instance.markSkippedTestAsBlocked}" />
    </f:entry>
    
    <f:entry title="${%Include test notes}" help="${rootURL}/../plugin/testlink/help-includeNotes.html">
        <f:checkbox name="TestNGClassNameResultSeeker.includeNotes" value="${instance.includeNotes}" checked="${instance.includeNotes}" />
    </f:entry>

</j:jelly>