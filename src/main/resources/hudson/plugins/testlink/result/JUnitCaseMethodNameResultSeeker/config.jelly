<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">

    <f:entry title="${%Include Pattern}" help="${rootURL}/../plugin/testlink/help-junitIncludePattern.html">
        <f:textbox id="includePattern" name="JUnitCaseMethodNameResultSeeker.includePattern" value="${instance.includePattern}" />
    </f:entry>

    <f:entry title="${%Key Custom Field}" help="${rootURL}/../plugin/testlink/help-atKeyCustomField-junit-method-name.html">
        <f:textbox id="keyCustomField" name="JUnitCaseMethodNameResultSeeker.keyCustomField" value="${instance.keyCustomField}" />
    </f:entry>
    
    <f:entry title="${%Attach JUnit XML}" help="${rootURL}/../plugin/testlink/help-attachResult.html">
        <f:checkbox name="JUnitCaseMethodNameResultSeeker.attachJUnitXML" value="${instance.attachJUnitXML}" checked="${instance.attachJUnitXML}" />
    </f:entry>

</j:jelly>
